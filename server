scp evisa.tar.gz root@[SERVER_IP]:.
sshfs root@[SERVER_IP]:/path/of/server/dir/ /path/of/local/dir/


apt-get install  php7.0-cli php7.0-common php7.0-curl php7.0-fpm php7.0-gd php7.0-json php7.0-mbstring php7.0-opcache php7.0-readline php7.0-xml php7.0-zip
tar -zxvf evisa.tar.gz
cd evisa
nano index.php
    // change SITE_URL
php composer.phar update
php -S [SERVER_IP]:8080
